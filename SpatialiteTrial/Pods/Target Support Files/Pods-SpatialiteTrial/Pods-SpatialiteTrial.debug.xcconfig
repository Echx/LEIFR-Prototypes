CLANG_CXX_LIBRARY = libstdc++
CLANG_ENABLE_MODULES = NO
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = ${PODS_ROOT}/geos/include ${PODS_ROOT}/geos/capi ${PODS_ROOT}/spatialite/src ${PODS_ROOT}/spatialite/src/headers ${PODS_ROOT}/geos/include ${PODS_ROOT}/geos/capi
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/geos.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/proj4.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/spatialite.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/sqlite3.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "geos" -framework "proj4" -framework "spatialite" -framework "sqlite3"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-SpatialiteTrial
PODS_ROOT = ${SRCROOT}/Pods